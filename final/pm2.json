{
    "apps": {
        "name": "expohunter",
        "script": "app.js",
        "cwd": "./",
        "watch": [
            "data",
            "routes",
            "public/js",
            "public/css",
            "views"
        ],
        "ignore_watch": [
            "node_modules",
            "logs",
            "public/image"
        ],
        "error_file": "./logs/app-err.log",
        "out_file": "./logs/app-out.log",
        "env": {
            "NODE_ENV": "production"
        }
    }
}
